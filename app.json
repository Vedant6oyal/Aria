{
  "expo": {
    "name": "Aria",
    "slug": "aria-music",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aria",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "description": "A beautiful music streaming app like Spotify",
    "primaryColor": "#1DB954",
    "ios": {
      "bundleIdentifier": "com.vg6sense.ariamusic",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "com.vg6sense.ariamusic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ],
      "expo-video",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "backgroundMode": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supabaseUrl": "https://bxhcphrtdfoicscsinqf.supabase.co",
      "supabaseAnonKey": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImJ4aGNwaHJ0ZGZvaWNzY3NpbnFmIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NDU3NDY0MzIsImV4cCI6MjA2MTMyMjQzMn0.O18XQStBUkah02qn1LZj0gKDNXpjHuSq4p-HSl4YreU",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3e45590c-fd51-43d0-9fc9-42531542b70d"
      }
    }
  }
}
