{
  "expo": {
    "name": "Aria",
    "slug": "aria-music",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aria",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "A beautiful music streaming app like Spotify",
    "primaryColor": "#1DB954",
    "ios": {
      "bundleIdentifier": "com.vg6sense.ariamusic",
      "supportsTablet": true
    },
    "android": {
      "package": "com.vg6sense.ariamusic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121212"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
